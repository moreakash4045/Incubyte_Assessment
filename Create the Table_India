%sql
-- Drop the existing Table_India if it exists
DROP TABLE IF EXISTS Table_India;

-- Create the Table_India
CREATE TABLE Table_India (
    Name VARCHAR(255),
    Cust_I VARCHAR(18),
    Open_Dt DATE,
    Consul_Dt DATE,
    VAC_ID CHAR(5),
    DR_Name VARCHAR(255),
    State CHAR(5),
    Country CHAR(5),
    DOB DATE,
    FLAG CHAR(1),
    Age INT,
    Days_Since_Last_Consulted INT
);
